{
  "manifest_version": 2,
  "version": "0.23",

  "name": "NoFame",
  "description": "Chrome extension to remove the names of terrible people",
  "author": "McCossum",

  "icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
  "content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["NoFame.js"],
			"run_at": "document_end"
		}
	]
}
